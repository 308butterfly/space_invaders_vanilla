{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 250,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": false,
    "cssEnable": [
        "css",
        "less",
        "scss"
    ]
}